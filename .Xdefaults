!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------

Xft.dpi:                    96
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

URxvt*cursorUnderline: 	    true
URxvt*cursorBlink: 	    true
URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
! URxvt.urgentOnBell:         true
! URxvt.visualBell:           true
URxvt.loginShell:           true
URxvt.saveLines:            5000
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Fonts
! URxvt.allow_bold:           true
! URxvt.font: -lucy-tewi2a-medium-r-normal--11-90-75-75-p-58-iso10646-1
 URxvt*font: xft:Monospace:pixelsize=16
 URxvt*boldFont: xft:Monospace:pixelsize=16

! Fix font space
URxvt*letterSpace: -1

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Perl extensions
URxvt.perl-ext-common:      default,matcher
URxvt.matcher.button:       1
URxvt.urlLauncher:          firefox

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #832e3d
*.color9:       #c23f53

! green
*.color2:       #759152
*.color10:      #a1c771

! yellow
*.color3:       #ba721c
*.color11:      #ff9c27

! blue
*.color4:       #2c6097
*.color12:      #3c84cf

! magenta
*.color5:       #a04cab
*.color13:      #dc68ea

! cyan
*.color6:       #35787b
*.color14:      #48a5a8

! white
